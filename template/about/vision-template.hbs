<h1>Our Vision</h1>
{{#each data}}
  <a href="?request=visions/{{id}}" class="text-decoration-none">
    <div class="clearfix">
      <img
        class="float-start me-3 rounded-4"
        src="{{thumbpath}}"
        alt="{{name}}"
      />
      <h3 class="text-black">{{name}}</h3>
      <p class="text-black-50 mb-3">{{summary}}</p>
    </div>
  </a>
{{/each}}